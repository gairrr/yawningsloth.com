---
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- for post in posts %}
  {%- if not post.fields.metadata.is404 %}
  <url>
    <loc>{{ post.fields.permalink | canonicalUrl }}</loc>
    <lastmod>{{ post.sys.updatedAt | machineReadableDate }}</lastmod>
  </url>
  {%- endif %}
  {%- endfor %}
</urlset>
